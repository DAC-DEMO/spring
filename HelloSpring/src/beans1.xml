<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="first" name="first" class="com.dac.spring1.Employee">
		<property name="id" value="1"></property>
	</bean>
	
	<bean id="second" name="second" class="com.dac.spring1.Employee" autowire-candidate="false">
		<property name="id" value="2"></property>
		<property name="name" value="Akshay Kumar"></property>
	</bean>

	<bean id="p1" name="p1" class="com.dac.spring1.Project" autowire="byType">
		<property name="dept" value="RnD"></property>
	</bean>
	
	
	

</beans>









